<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/bg_category_item_selected" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/bg_category_item_unselected" />
            <stroke android:width="1dp" android:color="@color/bg_divider" />
        </shape>
    </item>
</selector>